<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.king.business.mapper.ChapterMapper">
    <select id="list" resultType="com.king.business.domain.Chapter">
        SELECT c.id, c.name, c.course_id as courseId FROM `chapter` c
    </select>
</mapper>
